<!-- Bibliographic Level -->
<!ELEMENT BibLevel  (#PCDATA)>

<!-- Undefined Character Position -->
<!ELEMENT UCP  (#PCDATA)>

<!-- Indicator Count -->
<!ELEMENT IndCount  (#PCDATA)>

<!-- Subfield Code Count -->
<!ELEMENT SFCount  (#PCDATA)>

<!-- Base address of data -->
<!ELEMENT BaseAddr  (#PCDATA)>

<!-- Encoding Level -->
<!ELEMENT EncLevel  (#PCDATA)>

<!-- Descriptive Cataloging Form -->
<!ELEMENT DscCatFm  (#PCDATA)>

<!-- Linked record requirement -->
<!ELEMENT LinkRec  (#PCDATA)>

<!-- Entry Map -->
<!ELEMENT EntryMap  (FLength, SCharPos, IDLength, EMUCP)>

<!-- Length of the length-of-field portion -->
<!ELEMENT FLength  (#PCDATA)>

<!-- Length of the starting-character-position portion -->
<!ELEMENT SCharPos  (#PCDATA)>

<!-- Length of the implementation-defined portion -->
<!ELEMENT IDLength  (#PCDATA)>

<!-- Undefined entry map character position -->
<!ELEMENT EMUCP  (#PCDATA)>

<!-- Component Parts of Variable Fields -->
<!-- Variable Control Fields -->
