<!-- Character entity set. Typical invocation:
     <!ENTITY % ISOnum PUBLIC
       "ISO 8879:1986//ENTITIES Numeric and Special Graphic//EN">
     %ISOnum;
-->
<!ENTITY half   SDATA "1/2"--=fraction one-half-->
<!ENTITY frac12 SDATA "1/2"--=fraction one-half-->
<!ENTITY frac14 SDATA "1/4"--=fraction one-quarter-->
<!ENTITY frac34 SDATA "3/4"--=fraction three-quarters-->
<!ENTITY frac18 SDATA "1/8"--=fraction one-eighth-->
<!ENTITY frac38 SDATA "3/8"--=fraction three-eighths-->
<!ENTITY frac58 SDATA "5/8"--=fraction five-eighths-->
<!ENTITY frac78 SDATA "7/8"--=fraction seven-eighths-->

<!ENTITY sup1   SDATA "^1"--=superscript one-->
<!ENTITY sup2   SDATA "^2"--=superscript two-->
<!ENTITY sup3   SDATA "^3"--=superscript three-->

<!ENTITY plus   SDATA "+"--=plus sign B:-- >
<!ENTITY plusmn SDATA "+-"--/pm B: =plus-or-minus sign-->
<!ENTITY lt     SDATA "<"--=less-than sign R:-->
<!ENTITY equals SDATA "="--=equals sign R:-->
<!ENTITY gt     SDATA ">"--=greater-than sign R:-->
<!ENTITY divide SDATA "/"--/div B: =divide sign-->
<!ENTITY times  SDATA "*"--/times B: =multiply sign-->

<!ENTITY curren SDATA "[curren]"--=general currency sign-->
<!ENTITY pound  SDATA "[pound ]"--=pound sign-->
<!ENTITY dollar SDATA "$"--=dollar sign-->
<!ENTITY cent   SDATA "[cent  ]"--=cent sign-->
<!ENTITY yen    SDATA "[yen   ]"--/yen =yen sign-->

<!ENTITY num    SDATA "#"--=number sign-->
<!ENTITY percnt SDATA "%"--=percent sign-->
<!ENTITY amp    SDATA "&"--=ampersand-->
<!ENTITY ast    SDATA "*"--/ast B: =asterisk-->
<!ENTITY commat SDATA "@"--=commercial at-->
<!ENTITY lsqb   SDATA "["--/lbrack O: =left square bracket-->
<!ENTITY bsol   SDATA "\\"--/backslash =reverse solidus-->
<!ENTITY rsqb   SDATA "]"--/rbrack C: =right square bracket-->
<!ENTITY lcub   SDATA "{"--/lbrace O: =left curly bracket-->
<!ENTITY horbar SDATA "--"--=horizontal bar-->
<!ENTITY verbar SDATA "|"--/vert =vertical bar-->
<!ENTITY rcub   SDATA "}"--/rbrace C: =right curly bracket-->
<!ENTITY micro  SDATA "[micro ]"--=micro sign-->
<!ENTITY ohm    SDATA "[ohm   ]"--=ohm sign-->
<!ENTITY deg    SDATA "[deg   ]"--=degree sign-->
<!ENTITY ordm   SDATA "[ordm  ]"--=ordinal indicator, masculine-->
<!ENTITY ordf   SDATA "[ordf  ]"--=ordinal indicator, feminine-->
<!ENTITY sect   SDATA "[sect  ]"--=section sign-->
<!ENTITY para   SDATA "[para  ]"--=pilcrow (paragraph sign)-->
<!ENTITY middot SDATA "[middot]"--/centerdot B: =middle dot-->
<!ENTITY larr   SDATA "[larr  ]"--/leftarrow /gets A: =leftward arrow-->
<!ENTITY rarr   SDATA "[rarr  ]"--/rightarrow /to A: =rightward arrow-->
<!ENTITY uarr   SDATA "[uarr  ]"--/uparrow A: =upward arrow-->
<!ENTITY darr   SDATA "[darr  ]"--/downarrow A: =downward arrow-->
<!ENTITY copy   SDATA "[copy  ]"--=copyright sign-->
<!ENTITY reg    SDATA "[reg   ]"--/circledR =registered sign-->
<!ENTITY trade  SDATA "[trade ]"--=trade mark sign-->
<!ENTITY brvbar SDATA "[brvbar]"--=broken (vertical) bar-->
<!ENTITY not    SDATA "[not   ]"--/neg /lnot =not sign-->
<!ENTITY sung   SDATA "[sung  ]"--=music note (sung text sign)-->

<!ENTITY excl   SDATA "!"--=exclamation mark-->
<!ENTITY iexcl  SDATA "[iexcl ]"--=inverted exclamation mark-->
<!ENTITY quot   SDATA "\""--=quotation mark-->
<!ENTITY apos   SDATA "'"--=apostrophe-->
<!ENTITY lpar   SDATA "("--O: =left parenthesis-->
<!ENTITY rpar   SDATA ")"--C: =right parenthesis-->
<!ENTITY comma  SDATA ","--P: =comma-->
<!ENTITY lowbar SDATA "_"--=low line-->
<!ENTITY hyphen SDATA "-"--=hyphen-->
<!ENTITY period SDATA "."--=full stop, period-->
<!ENTITY sol    SDATA "[sol   ]"--=solidus-->
<!ENTITY colon  SDATA ":"--/colon P:-->
<!ENTITY semi   SDATA ";"--=semicolon P:-->
<!ENTITY quest  SDATA "?"--=question mark-->
<!ENTITY iquest SDATA "[iquest]"--=inverted question mark-->
<!ENTITY laquo  SDATA "`"--=angle quotation mark, left-->
<!ENTITY raquo  SDATA "'"--=angle quotation mark, right-->
<!ENTITY lsquo  SDATA "`"--=single quotation mark, left-->
<!ENTITY rsquo  SDATA "'"--=single quotation mark, right-->
<!ENTITY ldquo  SDATA "\""--=double quotation mark, left-->
<!ENTITY rdquo  SDATA "\""--=double quotation mark, right-->
<!ENTITY nbsp   SDATA " "--=no break (required) space-->
<!ENTITY shy    SDATA "'"--=soft hyphen-->
