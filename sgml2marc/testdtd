<!-- Author's Note:
     This DTD was designed for use in an on-line catalog employing SGML
     as its underlying record format.  Its design was intended to
     accomplish several objectives: 1. Identify (tag) content-bearing
     elements down to the subfield level; and 2. allow automatic conversion
     of USMARC records into this SGML format and back out again with
     as little loss of the original content as possible.
     Users should note that this DTD was designed for use with MARC
     records at the U.C. Berkeley campus, and therefore contains some
     elements for tagging of local holdings information that should 
     probably be removed/altered for use at other institutions.  It
     should also be noted that while I've done my best to ensure
     that format integration compliant records will work with this
     DTD, it also contains information for fields/subfields no longer
     considered valid under format integration in order to provide
     backwards compatability with pre-format integration MARC records.
     Use at your own risk. -->
    
<!ENTITY % doctype "USMARC" -- Document type generic identifier -->
<!ENTITY % ISOgrk1 SYSTEM "/usr2/cheshire2/sgml/charsets/ISOgrk1">
<!ENTITY % ISOgrk2 SYSTEM "/usr2/cheshire2/sgml/charsets/ISOgrk2">
<!ENTITY % ISOcyr1 SYSTEM "/usr2/cheshire2/sgml/charsets/ISOcyr1">
<!ENTITY % ISOcyr2 SYSTEM "/usr2/cheshire2/sgml/charsets/ISOcyr2">
<!ENTITY % ISOpub SYSTEM "/usr2/cheshire2/sgml/charsets/ISOpub">
<!ENTITY % ISOnum SYSTEM "/usr2/cheshire2/sgml/charsets/ISOnum">
<!ENTITY % ANSEL SYSTEM "/usr2/cheshire2/sgml/charsets/ANSEL">
<!-- %ISOgrk1;
%ISOgrk2;
%ISOcyr1;
%ISOcyr2;
%ISOpub;
%ISOnum;
%ANSEL
  -->

<!ELEMENT USMARC - - (Leader, Directry?, VarFlds)>
<!ATTLIST USMARC Material (BK|AM|CF|MP|MU|VM|SE) "BK"
                 id       CDATA    #IMPLIED>
<!-- Author's Note: the id attribute for the USMARC element is
                    intended to hold a unique record number for
                    each MARC record in the local database.  That
                    is to say, it is intended ONLY as an aid in
                    maintaining the local database of MARC records -->

<!ELEMENT Leader - O (LRL, RecStat, RecType, BibLevel, UCP, IndCount, SFCount,
                      BaseAddr, EncLevel, DscCatFm, LinkRec, EntryMap)>
<!ELEMENT Directry - O (#PCDATA)>
<!ELEMENT VarFlds - O (VarCFlds, VarDFlds)>

<!-- Component parts of Leader -->
<!-- Logical Record Length -->
<!ELEMENT LRL - O (#PCDATA)>

<!-- Record Status -->
<!ELEMENT RecStat - O (#PCDATA)>

<!-- Type of Record -->
<!ELEMENT RecType - O (#PCDATA)>

<!-- Bibliographic Level -->
<!ELEMENT BibLevel - O (#PCDATA)>

<!-- Undefined Character Position -->
<!ELEMENT UCP - O (#PCDATA)>

<!-- Indicator Count -->
<!ELEMENT IndCount - O (#PCDATA)>

<!-- Subfield Code Count -->
<!ELEMENT SFCount - O (#PCDATA)>

<!-- Base address of data -->
<!ELEMENT BaseAddr - O (#PCDATA)>

<!-- Encoding Level -->
<!ELEMENT EncLevel - O (#PCDATA)>

<!-- Descriptive Cataloging Form -->
<!ELEMENT DscCatFm - O (#PCDATA)>

<!-- Linked record requirement -->
<!ELEMENT LinkRec - O (#PCDATA)>

<!-- Entry Map -->
<!ELEMENT EntryMap - O (FLength, SCharPos, IDLength, EMUCP)>

<!-- Length of the length-of-field portion -->
<!ELEMENT FLength - O (#PCDATA)>

<!-- Length of the starting-character-position portion -->
<!ELEMENT SCharPos - O (#PCDATA)>

<!-- Length of the implementation-defined portion -->
<!ELEMENT IDLength - O (#PCDATA)>

<!-- Undefined entry map character position -->
<!ELEMENT EMUCP - O (#PCDATA)>

<!-- Component Parts of Variable Fields -->
<!-- Variable Control Fields -->
<!ELEMENT VarCFlds - O (Fld001, Fld003?, Fld005, Fld006*, Fld007*, Fld008)>

<!-- Variable Data Fields -->
<!ELEMENT VarDFlds - O (NumbCode, MainEnty?, Titles, EdImprnt?, PhysDesc?,
                        Series?, Notes?, SubjAccs?, AddEnty?, LinkEnty?,
                        SAddEnty?, HoldAltG?, Fld9XX?)>

<!-- Component Parts of Variable Data Fields -->
<!-- Numbers & Codes -->
<!ELEMENT NumbCode - O (Fld010?, Fld011?, Fld015?, Fld017*, Fld018?,
	                Fld019*, Fld020*,
                        Fld022*, Fld023*, Fld024*, Fld025*, Fld027*,
			Fld028*, Fld029*,
                        Fld030*, Fld032*, Fld033*, Fld034*, Fld035*, Fld036?,
                        Fld037*, Fld039*, Fld040?, Fld041?, Fld042?,
                	Fld043?, Fld044?,
                        Fld045?, Fld046?, Fld047?, Fld048*, Fld050*, Fld051*,
                        Fld052*, Fld055*, Fld060*, Fld061*, Fld066?,
                        Fld069*, Fld070*,
                        Fld071*, Fld072*, Fld074*, Fld080?, Fld082*,
			Fld084*, Fld086*, Fld088*, Fld096*)>

<!-- Main Entries -->
<!ELEMENT MainEnty - O (Fld100?, Fld110?, Fld111?, Fld130?)>

<!-- Titles -->
<!ELEMENT Titles - O (Fld210?, Fld211*, Fld212*, Fld214*, Fld222*,
	              Fld240?, Fld242*, Fld243?, Fld245, Fld246*, Fld247*)>

<!-- Edition, Imprint, etc. -->
<!ELEMENT EdImprnt - O (Fld250?, Fld254?, Fld255*, Fld256?, Fld257?, Fld260?,
                        Fld261?, Fld262?, Fld263?, Fld265?)>

<!-- Physical Description, etc. -->
<!ELEMENT PhysDesc - O (Fld300*, Fld305*, Fld306?, Fld310?, Fld315?,
	                Fld321*, Fld340*, Fld350?, Fld351*,
			Fld355*, Fld357*, Fld362*)>

<!-- Series Statements -->
<!ELEMENT Series - O (Fld400*, Fld410*, Fld411*, Fld440*, Fld490*)>

<!-- Notes -->
<!ELEMENT Notes - O (Fld500*, Fld501*, Fld502*, Fld503*, Fld504*,
	             Fld505?, Fld506*,
                     Fld507?, Fld508?, Fld510*, Fld511*,  Fld513*, Fld515*,
                     Fld516*, Fld518*, Fld520*, Fld521*, Fld522?,
	             Fld523?, Fld524?,
                     Fld525*, Fld530*, Fld533*, Fld534*, Fld535*,
	             Fld536*, Fld537?,
                     Fld538*, Fld540*, Fld541*, Fld544*, Fld545*, Fld546*,
                     Fld547*, Fld550*, Fld555*, Fld556*, Fld561*, Fld562*,
                     Fld565*, Fld567*, Fld570*, Fld580*, Fld581*, Fld582*,
                     Fld583*, Fld584*, Fld585*, Fld586*, Fld590*, Fld596*)>

<!-- Subject Access Fields -->
<!ELEMENT SubjAccs - O (Fld600*, Fld610*, Fld611*, Fld630*, Fld650*, Fld651*,
                        Fld653*, Fld654*, Fld655*, Fld656*, Fld657*, Fld658*,
                        Fld690*, Fld697*)>

<!-- Added Entries -->
<!ELEMENT AddEnty - O (Fld700*, Fld710*, Fld711*, Fld730*, Fld740*, Fld752*,
                       Fld753*, Fld754*, Fld755*)>

<!-- Linking Entries -->
<!ELEMENT LinkEnty - O (Fld760*, Fld762*, Fld765*, Fld767*, Fld770*, Fld772*,
                        Fld773*, Fld775*, Fld776*, Fld777*, Fld780*, Fld785*,
                        Fld787*, Fld797*)>

<!-- Series Added Entries -->
<!ELEMENT SAddEnty - O (Fld800*, Fld810*, Fld811*, Fld830*, Fld840*)>

<!-- Holdings, Alternate Graphics, Etc. -->
<!ELEMENT HoldAltG - O (Fld841*, Fld843*, Fld845*, Fld850*, Fld852*, Fld853*,
                        Fld854*, Fld855*, Fld856*, Fld863*, Fld864*, Fld865*,
			Fld866*, Fld867*, Fld868*, Fld876*, Fld877*, Fld878*,
			Fld880*, Fld886*)>

<!-- Local Holdings Information -->
<!-- Author's Note: This section may require additional fields to work -->
<!-- with your records -->
<!ELEMENT Fld9XX - O (Fld901|Fld902|Fld903|Fld905|Fld935|Fld950|Fld955|Fld998)*>

<!-- Component Parts of Variable Control Fields -->
<!-- Control Number -->
<!ELEMENT Fld001 - O (#PCDATA)>

<!-- Control Number Identifier -->
<!ELEMENT Fld003 - O (#PCDATA)>

<!-- Date and Time of Latest Transaction -->
<!ELEMENT Fld005 - O (#PCDATA)>

<!-- Linking Field -->
<!ELEMENT Fld006 - O (#PCDATA)>

<!-- Physical Description Fixed Field -->
<!ELEMENT Fld007 - o (#PCDATA)>

<!-- Fixed Length Data Elements -->
<!ELEMENT Fld008 - O (#PCDATA)>

<!-- Component Parts of Numbers & Codes -->
<!-- Library of Congress Control Number -->
<!ELEMENT Fld010 - O (a|b|z)*>
<!ATTLIST Fld010 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- Linking Library of Congress Control Number -->
<!ELEMENT Fld011 - O (a+)>
<!ATTLIST Fld011 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- National Bibliography Number -->
<!ELEMENT Fld015 - O (Six?, a+)>
<!ATTLIST Fld015 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- Copyright Registration Number -->
<!ELEMENT Fld017 - O (Six?, a+, b)>
<!ATTLIST Fld017 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- Copyright Article-Fee Code -->
<!ELEMENT Fld018 - O (a)>
<!ATTLIST Fld018 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- Field 019 is a local number field employed by U.C. Berkeley -->
<!-- cataloging records and does not constitute part of the USMARC -->
<!-- standard -->
<!ELEMENT Fld019 - O ((a|b|c|d|e|f|g|h|i|j|k|l|m|n|o|p|q|r|s|t|u|v|w|x|y|z)*)>
<!ATTLIST Fld019 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- International Standard Book Number -->
<!ELEMENT Fld020 - O (Six?, (a|c|z)*)>
<!ATTLIST Fld020 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- International Standard Serial Number -->
<!ELEMENT Fld022 - O (a|y|z)*>
<!ATTLIST Fld022 ISDSLvl     (ND|FULL|ABBREV) "ND"
                 I2          CDATA          #IMPLIED>

<!-- Standard Film Number -->
<!ELEMENT Fld023 - O (#PCDATA)>

<!-- Standard Recording Number or other standard identifier -->
<!ELEMENT Fld024 - O (Six?, (a|c|d|z|Two)*)>
<!ATTLIST Fld024 StdNum      (ISRC|UPC|ISMN|IAN|SICI|Two|Unspec|Blank)     #IMPLIED
                 DiffInd     (NoDiff|Diff|Blnk)         #IMPLIED>

<!-- Overseas Acquisition Number -->
<!ELEMENT Fld025 - O (a+)>
<!ATTLIST Fld025 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- Standard Technical Report Number -->
<!ELEMENT Fld027 - O (a, z*)>
<!ATTLIST Fld027 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- Publisher Number -->
<!ELEMENT Fld028 - O (Six?, a, b)>
<!ATTLIST Fld028 PubNmTyp    (ISSUE|MATRIX|PLATE|OTHER|VIDEO|Blank) #IMPLIED
                 NteAdEnty   (NN|YY|YN|NY|Blnk)  #IMPLIED>

<!-- Field 029 is a local numbers field employed by U.C. Berkeley -->
<!-- cataloging records and does not constitute part of the USMARC -->
<!-- standard -->
<!ELEMENT Fld029 - O (a|b|c|d|e|f|g|h|i|j|k|l|m|n|o|p|q|r|s|t|u|v|w|x|y|z)*>
<!ATTLIST Fld029 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- CODEN Designation -->
<!ELEMENT Fld030 - O (Six?, a, z*)>
<!ATTLIST Fld030 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- Postal Registration Number -->
<!ELEMENT Fld032 - O (Six?, a, b)>
<!ATTLIST Fld032 I1          CDATA          #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- Date/Time and Place of an event -->
<!ELEMENT Fld033 - O (Six?, (Three|a|b|c)*)>
<!ATTLIST Fld033 DateType    (NoDate|Single|Multiple|Range) #IMPLIED
                 EventTyp    (ND|Capture|Broadcast|Finding) #IMPLIED>

<!-- Coded Mathematical Data -->
<!ELEMENT Fld034 - O (Six?, (a|b|c|d|e|f|g|h|j|k|m|n|p)*)>
<!ATTLIST Fld034 ScaleTyp    (ND|Single|Range|Blank) #IMPLIED
                 I2          CDATA          #IMPLIED>

<!-- System Control Number -->
