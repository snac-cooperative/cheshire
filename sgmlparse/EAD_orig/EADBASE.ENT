<!-- ********************************************************** -->
<!--           ADJUNCT DESCRIPTIVE DATA (ADD) ELEMENTS          -->
<!-- ********************************************************** -->

<!--                             ADJUNCT DESCRIPTIVE DATA        
                                 A container element to hold
                                 information that is not 
                                 part of the archival description
                                 but that can facilitate the use 
                                 of the archival material or use 
                                 of the finding aid.
-->
<!ELEMENT add            - -     (head?, (%m.add;)*)             >
<!ATTLIST add            %a.global;                               
          encodinganalog CDATA                         #IMPLIED  >

<!--                             BIBLIOGRAPHY
                                 A list of works based on or about 
                                 the material that is described
                                 in the finding aid.
-->
<!ELEMENT bibliography   - -     (head?, (%m.blocks; | 
                                  %m.phrase.refs; | 
                                  bibliography)*)                >
<!ATTLIST bibliography   %a.global;                               
          numbered   (numbered | not)                  #IMPLIED    
          encodinganalog CDATA                         #IMPLIED  >

<!--                             BIBLIOGRAPHIC REFERENCE           
                                 A reference to a bibliographic
                                 entity such as a book, article,
                                 motion picture, etc.  This
                                 may be as simple as text(#PCDATA)
                                 or as complex as a citation entry 
                                 of the kind prescribed in the 
                                 Chicago Manual of Style. The named
                                 elements float in any order in the
                                 citation with or without 
                                 surrounding text.
-->
<!ELEMENT bibref         - -     (#PCDATA | %m.bibref;)* 
                                -(bibref)                         >
<!ATTLIST bibref         %a.global;                               
                         %a.ext.ptr;                              
          encodinganalog CDATA                         #IMPLIED   >

<!--                             EDITION                        -->
<!ELEMENT edition        - -     (#PCDATA | %m.phrase.bare;)*     >
<!ATTLIST edition        %a.global;                           
          encodinganalog CDATA                        #IMPLIED    > 

<!--                             BIBLIOGRAPHIC SERIES           -->
<!ELEMENT bibseries      - -     (#PCDATA | %m.phrase.bare; | 
                                   title | num)*                  >
<!ATTLIST bibseries      %a.global;                               
          encodinganalog CDATA                         #IMPLIED   >

<!--                             IMPRINT                        -->
<!ELEMENT imprint        - -     (#PCDATA | %m.phrase.bare; |  
                                  publisher | geogname | date)*   >
<!ATTLIST imprint        %a.global;                               
          encodinganalog CDATA                         #IMPLIED   >

<!--                             PUBLISHER                      -->
<!ELEMENT publisher      - -     (#PCDATA | %m.phrase.bare;)*     >
<!ATTLIST publisher      %a.global; 
          encodinganalog CDATA                        #IMPLIED    > 
     
<!--                             FILEPLAN
                                 The classification plan or scheme
                                 for the physical arrangement, 
                                 storage, and retrieval of files 
                                 that was used by the individual 
                                 or organization originally 
                                 responsible for creating or 
                                 compiling the materials.
-->
<!ELEMENT fileplan       - -     (head?, (%m.blocks; | 
                                  fileplan)*)                   >
<!ATTLIST fileplan       %a.global;                            
          encodinganalog CDATA                         #IMPLIED >


<!--                             RELATED MATERIAL
                                 Description or lists of related 
                                 materials.
-->
<!ELEMENT relatedmaterial 
                         - -     (head?, (%m.blocks; | 
                                  %m.phrase.refs; | 
                                  relatedmaterial)* )            >
<!ATTLIST relatedmaterial        %a.global;                     
          encodinganalog CDATA                         #IMPLIED  >

<!--                             ARCHIVAL REFERENCE
                                 An external reference to an 
                                 archival entity, such as a 
                                 collection or record group. 
-->
<!ELEMENT archref        - -     (#PCDATA | %m.phrase.basic; | 
                                  %m.did;)*           -(archref) >
<!ATTLIST archref        %a.global;
                         %a.ext.ptr;                             >


<!--                             SEPARATED MATERIAL
                                 Information about material that 
                                 has been separated or physically 
                                 removed by the archivist from 
                                 the material being described in 
                                 <archdesc>.
-->
<!ELEMENT separatedmaterial
                         - -     (head?, (%m.blocks; | 
                                  %m.phrase.refs; | 
                                  separatedmaterial)*)            >
<!ATTLIST separatedmaterial            
                        %a.global;                                
          encodinganalog CDATA                         #IMPLIED   >



<!-- ********************************************************** -->
<!--               INDEX ELEMENTS                               -->
<!-- ********************************************************** -->

<!--                             INDEX     
                                 Human-generated Index. 
                                 Provides means to create list of
                                 entries to the material that 
                                 cannot be derived directly from
                                 the material itself.
-->
<!ELEMENT index          - -     ( head?,(%m.blocks;)*, 
                                  ((((listhead)?), 
                                  indexentry+) | index*))         >
<!ATTLIST index          %a.global;                               
          encodinganalog CDATA                         #IMPLIED   >

<!--elem listhead        declared in List elements              -->

<!--                             INDEX ENTRY     
                                 One or more name or subject
                                 entries can be linked to one or
                                 more locations. 
-->
<!ELEMENT indexentry     - -     ((namegrp | %m.phrase.access;), 
                                  (ptrgrp | ptr | ref) )          >
<!ATTLIST indexentry     %a.global;                               >


<!--                             NAME GROUP 
                                 Allows grouping of name entries
                                 that share the same reference 
-->
<!ELEMENT namegrp        - -     (%m.phrase.access; | note)+      >
<!ATTLIST namegrp        %a.global;                               >

<!--                             POINTER GROUP
                                 Container element which holds
                                 Pointers <ptr>s.
-->
<!ELEMENT ptrgrp         - -     (ptr | ref)+                     >
<!ATTLIST ptrgrp         %a.global;                               >

<!-- elem ptr            declared in Links and Pointers         -->

<!-- ********************************************************** -->
<!--              BASIC STRUCTURAL ELEMENTS                     -->
<!-- ********************************************************** -->

<!--                             HEADING 
                                 Used for headings or titles of
                                 high and mid-level structures.
-->
<!ELEMENT head           - -     (#PCDATA | %m.phrase.bare;)*     >
<!ATTLIST head           %a.global; 
              --         Alternative/Short form of the heading  --
              althead    CDATA                        #IMPLIED    >

<!--                             PARAGRAPH                      -->
<!ELEMENT p              - -     (%para.content;)*                >
<!ATTLIST p              %a.global;                               >


<!--                             BLOCK QUOTE 
                                 Simple quotes use PCDATA.
                                 Block quotes may contain a 
                                 paragraph or other block 
                                 elements.
-->
<!ELEMENT blockquote     - -     (%m.blocks.noquote;)*           >
<!ATTLIST blockquote     %a.global;                              >


<!--                             NOTE 
                                 Used as both a summary note
                                 in <did> and broadly available 
                                 for general annotations.
-->
<!ELEMENT note           - -     (%m.blocks;)*                   >
<!ATTLIST note           %a.global;                              
          label          CDATA                         #IMPLIED
          displaytype    (reveal | present)            "present" 
          encodinganalog CDATA                         #IMPLIED  >


<!-- ********************************************************** -->
<!--              DATA (CONTENT) PHRASE ELEMENTS                -->
<!-- ********************************************************** -->

<!ELEMENT date           - -     (#PCDATA | %m.phrase.bare;)*     >
<!ATTLIST date           %a.global; 
          type           CDATA                         #IMPLIED 
          normal         CDATA                         #IMPLIED
          certainty      CDATA                         #IMPLIED 
          encodinganalog CDATA                         #IMPLIED   >
          
<!ELEMENT num            - -     (#PCDATA | %m.phrase.bare;)*     >
<!ATTLIST num            %a.global; 
          type           CDATA                         #IMPLIED 
          encodinganalog CDATA                         #IMPLIED   >
          
<!ELEMENT abbr           - -     (#PCDATA)                        >
<!ATTLIST abbr           %a.global; 
          expan          CDATA                         #IMPLIED   >
          
<!ELEMENT expan          - -     (#PCDATA)                        >
<!ATTLIST expan          %a.global; 
          abbr           CDATA                         #IMPLIED   >
          
<!ELEMENT address        - -     (addressline+)                   >
<!ATTLIST address        %a.global;                               >

<!ELEMENT addressline    - -     (#PCDATA | %m.phrase.bare;)*     >
<!ATTLIST addressline    %a.global;                               >
     
<!-- ********************************************************** -->
<!--              BARE PHRASE ELEMENTS (USED ALMOST ANYWHERE)   -->
<!-- ********************************************************** -->

<!--                             EMPHASIS (HIGHLIGHTING)
                                 Used to specify an alternative 
                                 rendering of a character, word,
                                 phrase, etc. within another
                                 element. For example, italics,
                                 bold, superscript, and subscript.
                                 Alternative rendering options are 
                                 given in a semi-closed list.
-->
<!ELEMENT emph           - -     (#PCDATA | %m.phrase.basic;)*
                                  -(emph)                         >
<!ATTLIST emph           %a.emph;
                         id            ID              #IMPLIED
                         altrender     CDATA           #IMPLIED   >

<!--                             LINEBREAK(for forced linebreak)-->
<!ELEMENT lb             - o     EMPTY                            >
     
<!-- ********************************************************** -->
<!--              DIGITAL ARCHIVAL OBJECTS ELEMENTS             -->
<!-- ********************************************************** -->

<!--                             DIGITAL ARCHIVAL OBJECT                 
                                 If there is no description, this 
                                 is an empty element; attributes
                                 point to the actual object.       
-->
<!ELEMENT dao            - -     (daodesc?)                        >
<!ATTLIST dao            %a.global;
                         %a.ext.ptr;                              >

<!--                             DIGITAL ARCHIVAL OBJECT 
                                 DESCRIPTION
                                 A prose description of the digital
                                 archival object so that it can be 
                                 read or otherwise interpreted for 
                                 the print-impaired.
-->
<!ELEMENT daodesc        - -     (%head.n.para;)                 >
<!ATTLIST daodesc        %a.global;                              >


<!--                             DIGITAL ARCHIVAL OBJECT GROUP              
                                 Used to group digital archival
                                 objects together with each 
                                 object explicitly referenced, 
                                 with or without accompanying 
                                 text.
-->
<!ELEMENT daogrp         - -     (daodesc?, dao+)                 >
<!ATTLIST daogrp         %a.global;                               >

     

<!-- ********************************************************** -->
<!--              LINKS AND POINTERS (See REFERENCES below)     -->
<!-- ********************************************************** -->


<!--                             POINTER
                                 Points to another location in the 
                                 same document and is EMPTY.
-->
<!ELEMENT ptr            - o     EMPTY                            >
<!ATTLIST ptr            %a.int.ptr;                              
                         %a.global;                               >
 

<!--                             EXTERNAL POINTER
                                 Points to a location in another
                                 document and is EMPTY.
-->
<!ELEMENT extptr         - o     EMPTY                            >
<!ATTLIST extptr         %a.ext.ptr;
                         %a.global;                               >


<!--                          NAME LOCATION ADDRESS
                             (for external link)
-->
<!ELEMENT nameloc       - -   (nmlist)+                           >
<!ATTLIST nameloc         
            --                Tells HyTime what this is         --
            HyTime      NAME            #FIXED       "nameloc"
            --                Required ID                       --
            id          ID                           #REQUIRED    >

<!--                          NAME LIST SPEC (for external link)-->
<!ELEMENT nmlist        - -   (#PCDATA)                           >
<!ATTLIST nmlist         
            --                Tell HyTime what this is          --
            HyTime      NAME            #FIXED       "nmlist"
            --                TYPE OF ITEM REFERENCED           --
            nametype    (element | entity | unified) "element"
            --                ENTITY                            --
            docorsub    ENTITY                       #IMPLIED    
            --                Optional ID                       --
            id          ID                           #IMPLIED     >

<!--                          HYTIME ILINK (Independent Link)   -->
<!ELEMENT ilink         - O   EMPTY                               >
<!ATTLIST ilink
            --                Tells HyTime engine type of link  --
            HyTime      NAME        #FIXED           "ilink"
            --                Place to which link points        --
            linkends    NMTOKENS                     #REQUIRED
            --                HYTime anchor roles               --
            anchrole    NMTOKENS                     #IMPLIED     
            --                External Access Traversal Rule    --
            extra       NMTOKENS                     #IMPLIED     
            --                Internal Access Traversal Rule    --
            intra       NMTOKENS                     #IMPLIED     
            --                Link End Term Information         --
            endterms    NMTOKENS                     #IMPLIED     
            --                Traversal of agglink Anchors      --
            aggtrav     NMTOKENS                     #IMPLIED   
            --                An optional ID                    --
            id          ID                           #IMPLIED     >

     
<!-- ********************************************************** -->
<!--              CROSS REFERENCES (SEE POINTERS ABOVE)         -->
<!-- ********************************************************** -->

<!--elem bibref                declared under Bibliography      --> 
<!--elem archref               declared under Related Material  -->

<!--                             REFERENCE
                                 Points to another location in the 
                                 same document and may have 
                                 content.  Includes refs to notes
                                 and footnotes. The type attribute 
                                 controls generated text, such as
                                 "See also".  The displaytype
                                 attribute controls whether the
                                 ref appears in the text (present)
                                 or is represented by an icon or 
                                 highlighted text (reveal). 
-->
<!ELEMENT ref            - -     (%para.content;)*         -(ref) >
<!ATTLIST ref            %a.int.ptr;
                         %a.global;
                         displaytype (reveal | present)            
                         "reveal"                                 >


<!--                             EXTERNAL REFERENCE
                                 Points to a location in another 
                                 document and may have 
                                 content.  The type attribute 
                                 controls generated text such as,
                                 "See also".  The displaytype
                                 attribute controls whether the
                                 ref appears in the text (present)
                                 or is represented by an icon or 
                                 highlighted text (reveal). 
-->
<!ELEMENT extref         - -     (%para.content;)*      -(extref) >
<!ATTLIST extref         %a.global;
                         %a.ext.ptr;                              >


<!--                             REFERENCE to a TITLE
                                 A reference to a title of another
                                 work cited within the text.  This 
                                 reference may contain just the
                                 name of a monograph, article, etc.
                                 or it may use the external pointer
                                 attributes to make a hyperlink
                                 to the cited document.
-->
<!ELEMENT title         - -     (#PCDATA | %m.phrase.bare; | 
                                 date | num)+                     >
<!ATTLIST title         %a.global;
                        %a.ext.ptr;                              
                        %a.emph;
              extent    (all | part |unknown-extent)  #IMPLIED
              pubstatus (pub | unpub | unknown-pub)   #IMPLIED    
              encodinganalog CDATA                    #IMPLIED    >
     
<!-- ********************************************************** -->
<!--              GENERAL PURPOSE LISTS                         -->
<!-- ********************************************************** -->


<!--                             LIST 
                                 Essential structure taken from TEI
                                 except <headLabel> and <headItem> 
                                 have been replaced with 
                                 <listhead>, <head01>, and 
                                 <head02> to make them generally 
                                 available for other list 
                                 structures in EAD.
-->
<!ELEMENT list           - -     (head?, 
                                 (item+ | (listhead?, defitem+))) >
<!ATTLIST list        %a.global;
          type       (deflist|simple|ordered|random)  #IMPLIED    >

<!--                             LIST HEAD                      
                                 Head element for two column 
                                 lists. Also used in chronlist
                                 and index.
-->          
<!ELEMENT listhead    - -     (head01?, head02?)                  >
<!ATTLIST listhead     %a.global;                                 >

<!--                          ITEM in a LIST                    -->
<!ELEMENT item        - -     (%para.content;)*                   >
<!ATTLIST item        %a.global;                                  >

<!ELEMENT defitem     - -     (label, item)                       >
<!ATTLIST defitem     %a.global;                                  >

<!--                          LABEL (TERM) IN A TWO-PART LIST   -->
<!ELEMENT label       - -     (#PCDATA | %m.phrase.plus;)*        >
<!ATTLIST label       %a.global;                                  >

<!--                          HEAD IN THE FIRST COLUMN OF A LIST-->
<!ELEMENT head01      - -     (#PCDATA | %m.phrase.bare;)*        >
<!ATTLIST head01      %a.global;                                  >

<!--                          HEAD IN THE SECOND COLUMN OF A 
                              LIST                              -->
<!ELEMENT head02      - -     (#PCDATA | %m.phrase.bare;)*        >
<!ATTLIST head02      %a.global;                                  >

