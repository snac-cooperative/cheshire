<!ELEMENT c05            - -     ((head?, did?, (%m.desc.elems;)*, 
                                   ( (thead?,c06+)* | dsc*))
                                  |
                                  (drow+, c06*)  )                 >

<!--                             COMPONENT LEVEL 6              -->
<!ELEMENT c06            - -     ((head?, did?, (%m.desc.elems;)*, 
                                   ( (thead?,c07+)* | dsc*))
                                  |
                                  (drow+, c07*)  )                 >

<!--                             COMPONENT LEVEL 7              -->
<!ELEMENT c07            - -     ((head?, did?, (%m.desc.elems;)*, 
                                   ( (thead?,c08+)* | dsc*))
                                  |
                                  (drow+, c08*)  )                 >

<!--                             COMPONENT LEVEL 8              -->
<!ELEMENT c08            - -     ((head?, did?, (%m.desc.elems;)*, 
                                   ( (thead?,c09+)* | dsc*))
                                  |
                                  (drow+, c09*)  )                 >

<!--                             COMPONENT LEVEL 9              -->
<!ELEMENT c09            - -     ((head?, did?, (%m.desc.elems;)*, 
                                   ( (thead?,c10+)* | dsc*))
                                  |
                                  (drow+, c10*)  )                 >

<!--                             COMPONENT LEVEL 10             -->
<!ELEMENT c10            - -     ((head?, did?, (%m.desc.elems;)*, 
                                   ( (thead?,c11+)* | dsc*))
                                  |
                                  (drow+, c11*)  )                 >

